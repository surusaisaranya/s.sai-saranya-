using namespace std;
#include <iostream>
int main()
{
int base,height,length,breadth,area_rec;
float area_tle;
cout<<"enter the values of base and height";
cin>>base>>height;
area_tle=0.5*base*height;
cout<<"area of triangle is:"<<area_tle;
cout<<"enter the values of length,breadth";
cin>>length>>breadth;
area_rec=length*breadth;
cout<<"area of rectangle is :"<<area_rec;

}
